# Show comments at right of disassembly if they fit in screen
e asm.cmtright=true

# Don't show the bytes in the middle
e asm.bytes = false

# Don't show pseudocode in disassembly. Eg mov eax, str.ok = > eax = str.ok
e asm.pseudo = false

# Display stack and register values on top of disasembly view (visual mode)
e cmd.stack = true

# Use UTF-8 to show cool arrows that do not look like crap :)
e scr.utf8 = true


# Set `mov` color to blue
ec mov rgb:000055